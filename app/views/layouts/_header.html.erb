<header>
  <% if user_signed_in? %>
    <li></li>
    <li>
      <%= link_to "Log out", destroy_user_session_path, method: :delete %>
    </li>
  <% elsif admin_signed_in? %>
    <li></li>
    <li>
      <%= link_to "Lot out", destroy_admin_session_path, method: :delete %>
    </li>
  <% else %>
    <li></li>
    <li>
      <%= link_to "Sign up", new_user_registration_path %>
    </li>
    <li>
      <%= link_to "Log in", new_user_session_path %>
    </li>
    <li>
      <%= link_to "Admin Log in", admin_session_path %>
    </li>
  <% end %>
</header>